				print (x,y,z)



for x in range(1,21):
    for y in range(1,34):
        for z in range(1,99):
            if (5 * x + 3 * y + z / 3) == 100:
                if (x + y + z) == 100:
                    if z % 3 == 0:
                        print x,y,z
